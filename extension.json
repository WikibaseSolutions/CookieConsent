{
  "name": "CookieConsent",
  "type": "other",
  "version": "0.1.0",
  "author": [
    "Marijn van Wezel"
  ],
  "url": "https://www.mediawiki.org/wiki/Extension:CookieConsent",
  "descriptionmsg": "cookieconsent-desc",
  "license-name": "GPL-2.0-or-later",
  "requires": {
    "MediaWiki": ">= 1.39.0",
    "platform": {
      "php": ">= 7.4.3"
    }
  },
  "MessagesDirs": {
    "CookieConsent": [
      "i18n"
    ]
  },
  "AutoloadNamespaces": {
    "MediaWiki\\Extension\\CookieConsent\\": "src/"
  },
  "HookHandlers": {
	"main": {
	  "class": "MediaWiki\\Extension\\CookieConsent\\Hooks"
	}
  },
  "Hooks": {
	"BeforePageDisplay": "main",
	"SkinAddFooterLinks": "main"
  },
  "ResourceModules": {
	"ext.CookieConsent": {
	  "localBasePath": "resources/ext.CookieConsent",
      "remoteExtPath": "CookieConsent/resources/ext.CookieConsent",
	  "scripts": [
		"ext.CookieConsent.js"
	  ],
	  "styles": [
		"ext.CookieConsent.css"
	  ],
	  "messages": [
		"cookieconsent-dialog-title",
		"cookieconsent-save-preferences",
		"cookieconsent-category-name-strictly-necessary",
		"cookieconsent-category-name-preference",
		"cookieconsent-category-name-analytical",
		"cookieconsent-category-name-advertising",
		"cookieconsent-category-desc-strictly-necessary",
		"cookieconsent-category-desc-preference",
		"cookieconsent-category-desc-analytical",
		"cookieconsent-category-desc-advertising",
		"cookieconsent-dialog-intro",
		"cookieconsent-dialog-outro"
	  ],
      "dependencies": [
		"mediawiki.cookie",
		"oojs-ui-core",
		"oojs-ui-widgets",
		"oojs-ui-windows"
	  ]
	}
  },
  "ServiceWiringFiles": [
    "src/CookieConsent.wiring.php"
  ],
  "load_composer_autoloader": true,
  "manifest_version": 2
}
